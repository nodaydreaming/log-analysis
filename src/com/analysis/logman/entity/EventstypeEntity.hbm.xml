<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.analysis.logman.entity.EventstypeEntity" table="eventstype" schema="logman">
        <id name="id">
            <column name="id" sql-type="int(2)"/>
            <generator class="increment"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="advice">
            <column name="advice" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>